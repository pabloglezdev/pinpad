{
  "name": "pinpad",
  "version": "1.0.0",
  "description": "Pinpad for securing some app",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabloglezdev/pinpad.git"
  },
  "scripts": {
    "sass:watch": "sass --watch src/styles:src/styles",
    "sass": "sass styles:dist/styles",
    "lint": "eslint .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\" --list-different",
    "validate": "pnpm run prettier & pnpm run lint",
    "prepare": "husky",
    "build": "webpack --config webpack.config.js",
    "babel": "babel src -d build"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@eslint/js": "9.8.0",
    "babel-loader": "^9.1.3",
    "css-loader": "7.1.2",
    "eslint": "9.8.0",
    "eslint-config-prettier": "9.1.0",
    "html-bundler-webpack-plugin": "3.17.3",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "9.1.4",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "sass-loader": "16.0.0",
    "svg-loader": "0.0.2",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "keywords": [],
  "author": "Pablo González González <pablo.glez.dev@gmail.com>",
  "homepage": "https://github.com/pabloglezdev/pinpad#readme"
}
